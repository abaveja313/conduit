{
  "buildCommand": "cd ../.. && pnpm turbo run build --filter=web...",
  "installCommand": "cd ../.. && pnpm install --frozen-lockfile",
  "framework": "nextjs",
  "outputDirectory": ".next",
  "headers": [
    {
      "source": "/workers/(.*)\\.wasm",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/wasm"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/telemetry-x7f9/static/(.*)",
      "destination": "https://us-assets.i.posthog.com/static/$1"
    },
    {
      "source": "/telemetry-x7f9/decide",
      "destination": "https://us.i.posthog.com/decide"
    },
    {
      "source": "/telemetry-x7f9/(.*)",
      "destination": "https://us.i.posthog.com/$1"
    }
  ],
  "crons": []
}
